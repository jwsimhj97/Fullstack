package com.hi;

import java.net.MalformedURLException;
import java.net.URL;
import java.net.URLConnection;

public class Ex02 {

	public static void main(String[] args) {
		URL url=null;
		try {
			url=new URL("https://google.com");
			URLConnection conn=url.URLConnection();
			
		} catch (MalformedURLException e) {
			e.printStackTrace();
		}
	}

}
