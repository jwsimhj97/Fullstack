package com.hi;

import java.io.*;

public class Ex03 {

	public static void main(String[] args) {
		OutputStream os=null;
		ObjectOutputStream oos=null;
		
		try {
			os=new FileOutputStream("test02.bin");
			oos=new ObjectOutputStream(os);
			oos.writeObject("¹®ÀÚ¿­");
			
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}finally{
			try {
				oos.close();
				os.close();
			} catch (IOException e) {
				e.printStackTrace();
			}
		}
	}

}
