package com.web;

import java.io.IOException;
import java.net.ServerSocket;
import java.net.Socket;

public class Server {

	public static void main(String[] args) {
		ServerSocket serve=null;
		Socket sock=null;
		try {
			serve=new ServerSocket(8080);
			sock=serve.accept();
			System.out.println(sock);
		} catch (IOException e) {
			e.printStackTrace();
		}finally{
			serve.close();
		}
	}

}
