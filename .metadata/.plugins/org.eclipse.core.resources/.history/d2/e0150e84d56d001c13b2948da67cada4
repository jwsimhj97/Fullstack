package com.hi;

import java.io.*;

public class Ex06 {

	public static void main(String[] args) {
		InputStream is=null;
		Reader isr=null;
		
		try {
			is=new FileInputStream("test05.txt");
			isr=new InputStreamReader(is);
			
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		}finally{
			try {
				is.close();
			} catch (IOException e) {
				e.printStackTrace();
			}
		}
	}

}
