package com.hi;

public class Ex12 {

	public static void main(String[] args) {
		// 로또번호생성기 (ver 0.3.0)
		Set set=new java.util.TreeSet();
//		set.add(ran);
		
		while(set.size()!=6){				
			int ran=(int)(Math.random()*45)+1;
		}

		Iterator ite=set.iterator();
		while(ite.hasNext()){
			System.out.print(ite.next()+" ");
		}
		
//		System.out.println(set);
	}

}
