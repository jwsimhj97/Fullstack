package com.hi;

import java.awt.Frame;
import java.awt.MenuBar;

public class Ex09 extends Frame{

	public Ex09(){
		MenuBar mb=new MenuBar();
		Menu mn=new Menu();
		mb.add(mn);
		
		setMenuBar(mb);
		
		setBounds(100,100,400,300);
		setVisible(true);
	}
	
	public static void main(String[] args) {
		new Ex09();
	}

}
