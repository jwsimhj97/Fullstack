// ArrayList
public class Ex05 {

	public static void main(String[] args) {
		java.util.ArrayList list=new ArrayList();
		list.add("첫번째");
//		list.add("두번째");
//		list.add("세번째");
//		list.add("네번째");
		
		System.out.println(list.get(0));
		System.out.println(list.get(1));
		System.out.println(list.get(2));
		System.out.println(list.get(3));
	}

}
