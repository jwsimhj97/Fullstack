package com.hi;

public class Ex12 {

	public static void main(String[] args) {
		// 로또번호생성기 (ver 0.3.0)
		java.util.Set set=new java.util.TreeSet();
		int ran=(int)(Math.random()*45)+1;
		set.add(ran);
		
		java.util.Iterator ite=set.iterator();
		while(set.size()!=6){				
			java.util.Iterator ite=set.iterator();
			System.out.println(ite);
		}
		
		
//		System.out.println(set);
	}

}
