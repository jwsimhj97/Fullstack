package com.room802;

public class Ex07 {

	public static void main(String[] args) {
//		System.out.println(1);
//		System.out.println(23);
//		System.out.println(456);
		int temp=1;
		for(int y=0; y<3; y++){
			temp+=y;
			for(int x=temp; x<=1; x++){
				System.out.print(i);
			}
			System.out.println();
		}
			
		System.out.println("-------------");
		
		for(int i=1; i<=1; i++){
			System.out.print(i);
		}
		System.out.println();
		for(int i=1+1; i<=1+1+1; i++){
			System.out.print(i);
		}
		System.out.println();
		for(int i=1+1+2; i<=1+1+2+2; i++){
			System.out.print(i);
		}
		System.out.println();
		for(int i=1+1+2+3; i<=1+1+2+2+3+3; i++){
			System.out.print(i);
		}
		System.out.println();
		
	}

}
