package com.hi;
// Ελ½Ε
import java.net.*;

public class Ex16 {

	public static void main(String[] args) {
		InetAddress net=null;
		
		try {
			net=InetAddress.getByName("naver.com");
			
			System.out.println(net.getHostName());
			System.out.println(net.getHostAddress());
		} catch (UnknownHostException e) {
			e.printStackTrace();
		}
	}

}
