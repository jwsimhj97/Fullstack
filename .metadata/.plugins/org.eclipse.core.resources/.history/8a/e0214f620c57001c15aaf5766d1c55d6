package com.room802;

public class Ex07 {

	public static void main(String[] args) {
//		System.out.println(1);
//		System.out.println(23);
//		System.out.println(456);
		int i=1;
		for(int j=0; j<3; j++){
			for(i=i+j; i<=1; i++){
				System.out.print(i);
			}	
			System.out.println();
		}
			
		System.out.println("-------------");
		
		for(int i=1; i<=1; i++){
			System.out.print(i);
		}
		System.out.println();
		for(int i=1+1; i<=1+1+1; i++){
			System.out.print(i);
		}
		System.out.println();
		for(int i=1+1+2; i<=1+1+2+2; i++){
			System.out.print(i);
		}
		System.out.println();
		for(int i=1+1+2+3; i<=1+1+2+2+3+3; i++){
			System.out.print(i);
		}
		System.out.println();
		
	}

}
