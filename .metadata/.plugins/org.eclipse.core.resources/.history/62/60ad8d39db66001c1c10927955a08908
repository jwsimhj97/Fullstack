package com.hi;

public void run(){
	System.out.println("new Thread start...");
	System.out.println("thread end...");
}

public class Ex06 {

	public static void main(String[] args) {
		System.out.println("main start...");
		Thread thr=new Thread();
		thr.start();
		System.out.println("main end...");
	}

}
