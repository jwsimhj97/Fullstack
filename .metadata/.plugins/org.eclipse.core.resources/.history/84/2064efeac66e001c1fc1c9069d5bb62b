package com.hi.step03;

import java.net.*;
import java.io.*;


public class Server {

	public static void main(String[] args) {
		ServerSocket serve=null;
		
		Socket sock=null;
		InputStream is=null;
		OutputStream os=null;
		InputStreamReader isr=null;
		OutputStreamWriter osw=null;
		BufferedReader br=null;
		BufferedWriter bw=null;
		
		try {
			serve=new ServerSocket(5001);
			sock=serve.accept();
			
		} catch (IOException e) {
			e.printStackTrace();
		}finally{
			try {
				bw.close();
				br.close();
				osw.close();
				isr.close();
				os.close();
				is.close();
				sock.close();
				
				serve.close();
			} catch (IOException e) {
				e.printStackTrace();
			}
		}
	}

}
