package com.hi;

import java.io.File;

public class Ex10 {

	public static void main(String[] args) {
		// IO
		// 경로
		// 절대경로 상대경로
		java.io.File file=new File(".\\src\\com\\hi\\Ex10.java");
		System.out.println(file.exists());		// 파일이 존재하는지
		System.out.println(file.getPath());
		System.out.println(file.getAbsolutePath());
	}

}
