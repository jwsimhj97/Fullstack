package com.hi;

public class Ex12 extends Thread{
	
	@Override
	public void run() {
		System.out.println(getName()+" start...");
		
		try {
			wait();
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		
		System.out.println(getName()+" end...");
	}

	public static void main(String[] args) {
		
	}

}
