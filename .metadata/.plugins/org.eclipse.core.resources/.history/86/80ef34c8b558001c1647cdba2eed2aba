package com.hi;

public class Ex06 {
	public static int c;
	public static int d;

	public static void func01(int a, int b){
		System.out.println(a+b);
	}
	
	public static void func02(){
		int a=10;
		int b=5;
		System.out.println(c+d);
	}
	
	public static void main(String[] args) {
		int a,b;
		a=2;
		b=3;
		func01(a,b);
		c=a;
		d=b;
		func02();
	}

}
