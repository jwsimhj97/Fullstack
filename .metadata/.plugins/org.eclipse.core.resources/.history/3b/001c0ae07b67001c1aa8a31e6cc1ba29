package com.hi;

import java.io.File;
import java.io.FileInputStream;
import java.io.FileNotFoundException;

public class Ex03 {

	private static FileInputStream is;

	public static void main(String[] args) {
		File file=new File("test01.bin");
		if(file.exists()){
			try {
				FileInputStream inn=new java.io.FileInputStream(file);
				int su=is.read();
				System.out.println(su);
				is.close();
			} catch (FileNotFoundException e) {
				e.printStackTrace();
			}
		}
	}

}
