package com.hi;

public class Ex09 extends Thread{

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	@Override
	public void run() {
		while(true){
			System.out.println(getName()+" run...");
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}

}
