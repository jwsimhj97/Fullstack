package com.hi;

import java.io.File;
import java.io.IOException;

public class Ex07 {

	public static void main(String[] args) {
		// 문자열 스트림
		File file=new File("test01.txt");
		if(!file.exists()){
			try {
				file.createNewFile();
			} catch (IOException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}

}
