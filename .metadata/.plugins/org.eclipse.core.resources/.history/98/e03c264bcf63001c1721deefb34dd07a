package com.hi;

public class Ex12 {

	public static void main(String[] args) {
		// 로또번호생성기 (ver 0.3.0)
		java.util.Set set=new java.util.TreeSet();
		int ran=(int)(Math.random()*45)+1;
		System.out.println(ran);
		
		for(int i=0; i<45; i++){
			set.add(i+1);
		}
		
//		System.out.println(set);
	}

}
