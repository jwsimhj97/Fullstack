package com.hi.step01;

import java.io.IOException;
import java.net.*;

public class Server {

	public static void main(String[] args) {
		ServerSocket serv=null;
		Socket sock=null;
		try {
			serv=new ServerSocket(5000);
			sock=serv.accept();
			
		} catch (IOException e) {
			e.printStackTrace();
		}finally{
			try {
				sock.close();
				serv.close();
			} catch (IOException e) {
				e.printStackTrace();
			}
		}
	}

}
