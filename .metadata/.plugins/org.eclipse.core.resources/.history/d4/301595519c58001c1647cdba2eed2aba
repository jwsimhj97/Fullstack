package com.hi;

public class Ex02 {
	public static void func01(){
		System.out.println("static method func01...");
	}
	public void func02(){
		System.out.println("non-static method func02...");
	}
	public void func03(){
		System.out.println("non-static method func03...");
	}

	public static void main(String[] args) {
		func01();
		Ex02 me=new Ex02();
		me.func02();
		Ex02 me2=new Ex02();
		me2.func03();
	}

}
