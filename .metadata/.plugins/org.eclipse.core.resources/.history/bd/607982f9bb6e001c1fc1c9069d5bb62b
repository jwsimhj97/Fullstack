package com.hi.step02;

import java.io.*;
import java.net.*;

public class Client {

	public static void main(String[] args) {
		Socket sock=null;
		InputStream is=null;
		OutputStream os=null;
		
		try {
			sock=new Socket("localhost",5000);
		} catch (UnknownHostException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}finally{
			try {
				sock.close();
			} catch (IOException e) {
				e.printStackTrace();
			}
		}
	}

}
