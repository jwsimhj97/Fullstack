package com.hi;

import java.io.File;
import java.io.FileNotFoundException;

public class Ex03 {

	public static void main(String[] args) {
		File file=new File("test01.bin");
		if(file.exists()){
			try {
				new java.io.FileInputStream(file);
			} catch (FileNotFoundException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}

}
