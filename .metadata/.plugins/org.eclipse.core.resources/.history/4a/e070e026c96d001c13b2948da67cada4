package com.hi;

import java.io.*;

public class Ex03 {

	public static void main(String[] args) {
		OutputStream os=null;
		ObjectInputStream oos=null;
		
		try {
			os=new FileOutputStream("test02.bin");
			oos=new ObjectInputStream(os);
			
		} catch (FileNotFoundException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}finally{
			try {
				os.close();
			} catch (IOException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}

}
